{
  "name": "react-countdown",
  "version": "3.0.0-beta",
  "description": "A customizable countdown component for React.",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ndresx/react-countdown.git"
  },
  "author": "Martin Veith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndresx/react-countdown/issues"
  },
  "homepage": "https://github.com/ndresx/react-countdown",
  "keywords": [
    "react",
    "typescript",
    "countdown",
    "component",
    "hook"
  ],
  "scripts": {
    "build": "yarn build:clean && yarn rollup --config",
    "build:clean": "rm -rf ./dist mkdir dist",
    "build:watch": "yarn build:clean && yarn rollup --config --watch",
    "clean": "rm -rf ./node_modules && rm -rf ./coverage && yarn build:clean",
    "eslint": "eslint ./src/**/*.{ts,tsx} --quiet",
    "eslint:fix": "yarn eslint --fix",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:eslint": "yarn eslint",
    "lint:prettier": "yarn prettier",
    "lint:tsc": "tsc --noEmit",
    "prettier": "prettier --list-different ./src/**/*.{ts,tsx}",
    "prettier:fix": "prettier --write ./src/**/*.{ts,tsx}",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.9.22",
    "@types/testing-library__react-hooks": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  }
}
